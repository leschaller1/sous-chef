{
  "name": "Sous Chef",
  "description": "",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "main.css"
      ],
      "js": [
        "main.js"
      ]
    }
  ],
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "scripting",
    "webNavigation"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/cookie-16x16.png",
      "32": "/images/cookie-32x32.png",
      "48": "/images/cookie-48x48.png",
      "128": "/images/cookie-180x180.png"
    }
  },
  "icons": {
    "16": "/images/cookie-16x16.png",
    "32": "/images/cookie-32x32.png",
    "48": "/images/cookie-48x48.png",
    "128": "/images/cookie-180x180.png"
  }
}